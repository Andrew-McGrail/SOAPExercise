<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Bluesource_Regression_demo" parallel= "methods" thread-count="10">
	<listeners>
		<listener class-name="com.orasi.utils.Screenshot" />
	</listeners>
	
	<test name="Chrome">
		<parameter name="browserUnderTest" value="chrome" />
		<parameter name="environment" value="qa" />
		<parameter name="runLocation" value="sauce" />
		<parameter name="browserVersion" value="" />
		<parameter name="operatingSystem" value="Windows 7" />
		<groups>
			<run>
				<include name="demo" />		
			</run>
		</groups>
		<packages>
			<package name="com.orasi.bluesource.features.*">
			</package>
		</packages>
	</test> <!-- Test -->

</suite> <!-- Suite -->